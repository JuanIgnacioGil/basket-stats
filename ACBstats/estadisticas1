   if (temporada == '19421943')
   {      codigos.options[0] = new Option("VII Copa del Generalísimo");
      codigos.options[0].value = 'CREY-7';
   }
   if (temporada == '19411942')
   {      codigos.options[0] = new Option("VI Copa del Generalísimo");
      codigos.options[0].value = 'CREY-6';
   }
   if (temporada == '19401941')
   {      codigos.options[0] = new Option("V Copa del Generalísimo");
      codigos.options[0].value = 'CREY-5';
   }
   if (temporada == '19391940')
   {      codigos.options[0] = new Option("IV Copa del Generalísimo");
      codigos.options[0].value = 'CREY-4';
   }
   if (temporada == '19351936')
   {      codigos.options[0] = new Option("II Campeonato de España");
      codigos.options[0].value = 'CREY-2';
      codigos.options[1] = new Option("III Campeonato de España");
      codigos.options[1].value = 'CREY-3';
   }
   if (temporada == '19331934')
   {      codigos.options[0] = new Option("I Campeonato de España");
      codigos.options[0].value = 'CREY-1';
   }
   }
   codigos.options[0].selected = true;
}
</script>
